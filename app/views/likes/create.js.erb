<% if @like.errors.any? %>
  // Render new review form with errors
  $(".like-div[data-id="+<%=@art_piece.id%>+"]").html("<%= j render 'likes/form', art_piece: @art_piece, like: @like, user:current_user %>");
<% else %>
  console.log('pas derreur')
  $(".like-div[data-id="+<%=@art_piece.id%>+"]").html("<%= j render 'likes/delete', art_piece: @art_piece, like: @like, user: current_user %>");
<% end %>